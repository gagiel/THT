#content .denglu{
	float:right;
	padding-right:30px;
	line-height:35px;
	color:#FFF;
	z-index:999;
}
#denglukuang{
 height:35px;
}
#content_inner {
 margin:0 auto;
 width:1200px;
 height:59px;
 position:relative;
} 
#tht {
 padding-top:53px;
 margin:0 auto;
 width:309px;
 padding-bottom:20px;
} 
#content .img {
 position:absolute;
 left:379px;
 top:14px;
 z-index:1000;
}
.searchselect {
 position:absolute;
 top:3px;
 left:283px;
 width:120px;
 z-index:999;
}
#search {
 padding-left:280px;
 position:relative;
}
.search_text {
 width:406px;
 height:33px;
 float:left;
 border:#19294b 1px solid;
 padding-left:130px;
 line-height:33px;
}
.search_btn {
 width:105px;
 height:35px;
 float:left;
 vertical-align:top;
 background-color:#19294b;
 border:0;
 color:#FFF;
 font-size:16px;
}

.searchselect span {
	float:left;
	width:120px;
	background-color:#b3b3b3;
	text-align:center;
	line-height:29px;
}
.searchselect p {
	float:left;
	color:#19294b;
	padding-left:22px;
}
.searchselect ul {
	display: none;
	z-index: 150;
	overflow:hidden;
	width:120px;
}
.searchselect ul li {
	float:left;
	width:120px;
	height:30px;
	text-align:center;
	background-color:#e0dfdf;
}
.searchselect ul li a {
	float:left;
	width:120px;
	line-height:24px;
	color:#666666;
	text-decoration:none;
}
.searchselect ul li a:hover {
	background:#fff;
	color: #19294b;
	text-decoration:none;
}
#nav {
	background-image:url(../images/line_10.jpg);
	height:150px;
	min-width:1200px;
}
#nav_inner {
	height:166px;
	padding-top:26px;
	position:relative;
    width:100%;
    text-align:center;
	overflow: hidden;
}
#nav_inner ul {
    float:left;
    position:relative;
    left:50%;
}
#nav_inner li {
	position:relative;
    right:50%;
    float:left;
    margin-left:12px;
	margin-right:12px;
}
#nav_inner a{
	display:block;
	width:91px;
	height:105px;
	overflow:hidden;	
}
.xitong,.xitong:hover{
	background-image:url(../images/xitong_03.jpg);
}
.gongzuo,.gongzuo:hover{
	background-image:url(../images/gongzuo.jpg);
}
.lianxiren,.lianxiren:hover{
	background-image:url(../images/lianxiren.jpg);
}
.news,.news:hover{
	background-image:url(../images/news.jpg);
}
.xiaoxi,.xiaoxi:hover{
	background-image:url(../images/xinxi.jpg);
}
.huodong,.huodong:hover{
	background-image:url(../images/huodong.jpg);
}
.zhuanti,.zhuanti:hover{
	background-image:url(../images/zhuanti.jpg);
}
.xitong:hover,.gongzuo:hover,.lianxiren:hover,.news:hover,.xiaoxi:hover,.huodong:hover,.zhuanti:hover{
	background-position:right;
}
#logo{
	height:225px;
	background-color:#FFF;
}
#logo_inner {
 width:1200px;
 height:100%;
 margin:0 auto;
 background-color:#FFF;
}

#logo_inner h1 {
	text-align:center;
	font-size:18px;
	font-family:"微软雅黑";
	font-weight:normal;
	color:#5d5d5d;
}
#logo_inner p {
	text-align:center;
	font-size:10px;
	line-height:35px;
	color:#5d5d5d;
	font-family:Arial, Helvetica, sans-serif;
}

#d_tab29 ul, #d_tab29 li {
	margin:0;
	padding:0;
	list-style:none
}
#d_tab29 img {
	border:0;
	vertical-align:top
}
#d_tab29 {
	position:relative;
	height:167px;
	text-align:left
}
#d_tab29 .d_img {
	position:relative;
	margin:0 auto;
	width:868px;
	height:100%
}
#d_tab29 .d_img li {
	position:absolute;
	display:none;
	z-index:0
}
#d_tab29 .d_img li img {
	width:100%
}
#d_tab29 .d_img li.d_pos1 {
	display:block;
	width:178px;
	left:0%;
	top:45px;
	z-index:1
}
#d_tab29 .d_img li.d_pos2 {
	display:block;
	width:228px;
	left:15%;
	top:35px;
	z-index:2
}
#d_tab29 .d_img li.d_pos3 {
	display:block;
	width:301px;
	left:32.5%;
	top:15px;
	z-index:3
}
#d_tab29 .d_img li.d_pos4 {
	display:block;
	width:228px;
	right:15%;
	top:35px;
	z-index:2
}
#d_tab29 .d_img li.d_pos5 {
	display:block;
	width:178px;
	right:0%;
	top:45px;
	z-index:1
}
#d_tab29 .d_menu {
	position:absolute;
	bottom:20px;
	width:100%;
	text-align:center
}
#d_tab29 .d_menu li {
	display:inline-block;
	zoom:1;
	background:url('img/btn.png') no-repeat 0 0;
	width:12px;
	height:12px;
	cursor:pointer;
	margin-right:3px;
}
#d_tab29 .d_menu li.d_select {
	background:url('img/btn.png') no-repeat 0 -18px;
}
#d_tab29 .d_next {
	position:absolute;
	left:50%;
	margin-left:450px;
	top:45px;
	z-index:10;
	cursor:pointer
}
#d_tab29 .d_prev {
	position:absolute;
	left:50%;
	margin-left:-510px;
	top:45px;
	z-index:10;
	cursor:pointer
}
#footer p{
	text-align:center;
	line-height:50px;
	color:#5d5d5d;	
}

