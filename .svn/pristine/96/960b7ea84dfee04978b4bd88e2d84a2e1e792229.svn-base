@charset "utf-8";
/* CSS Document */

* {
	margin: 0;
	padding: 0;
}
body {
	font-family:"微软雅黑";
	font-size: 14px;
	background-color:#e0dfdf;
}
.clear {
	clear: both;
}
ul {
	list-style: none;
}
.img {
	border: 0;
}
a img {
	border:none
} 
a:focus{
outline:none;
}
.black_overlay {
	display:none;
	position:absolute;
	top:0%;
	left:0%;
	width:100%;
	height:100%;
	background-color:black;
	z-index:1001;
	-moz-opacity:0.8;
	opacity:.80;
	filter:alpha(opacity=80);
}
.white_content {
	display:none;
	position:absolute;
	margin:0 auto;
	width:500px;
	height:400px;
	background-color:white;
	z-index:1002;
	overflow:auto;
	top:50%;
	left:50%;
	margin:-150px 0 0 -200px;
	border:1px solid #f0f0f0;
	background: url(../images/login_bg.png) no-repeat bottom center;
}
.white_content h3 { position: relative; margin-bottom: 65px; width: 500px; height: 30px; background: #465c8c; line-height: 30px; text-align: center; color: #fff; font-size: 14px; }
.white_content h3 a { position: absolute; top: 0; right: 5px; color: #bec9e3; text-decoration: none; }
.white_content h3 a:hover { color: #fff; }
.white_content .cwnr1 { margin: 0 auto; width: 80%; height: 100px; text-align: center; font-size: 18px; color: #c00; font-weight: bold; }
.white_content input.log_bnt { }
#content {
	background:url(../images/bgdt_01.jpg) repeat-y center top;
	height:280px;
	min-width:1200px;
}
