/*分页部件*/
.sabrosus { padding: 3px; margin: 3px; text-align: center; font-size: 12px; width: 90%; }
.sabrosus input { width: 40px; height: 20px; border: #9aafe5 1px solid; color: #2e6ab1; }
.sabrosus a { margin-right: 2px; padding: 2px 5px; border: #9aafe5 1px solid; color: #2e6ab1; text-decoration: none; }
.sabrosus a:hover { border: #2b66a5 1px solid; background-color: lightyellow; color: #000; }
.pagination a:active { border: #2b66a5 1px solid; background-color: lightyellow; color: #000; }
.sabrosus span.current { margin-right: 2px; padding: 2px 5px; border: navy 1px solid; color: #fff; font-weight: bold; background-color: #2e6ab1; }
.sabrosus span.disabled { border: #929292 1px solid; margin-right: 2px; padding: 2px 5px; color: #929292; }
